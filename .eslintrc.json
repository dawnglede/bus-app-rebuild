{
  "extends": ["next/core-web-vitals", "prettier"],
  "rules": {
    "no-unused-vars": "warn",
    "quotes": ["warn", "single"],
    "no-console": "warn",
    "space-before-function-paren": [
      "error",
      { "anonymous": "always", "named": "never", "asyncArrow": "always" }
    ],
    "semi": ["warn", "never"]
  },
  "parserOptions": {
    "ecmaVersion": 11
  }
}
